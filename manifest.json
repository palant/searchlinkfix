{
  "manifest_version": 2,
  "version": "1.6.8",
  "minimum_chrome_version": "49.0",
  "minimum_opera_version": "36.0",
  "applications": {
    "gecko": {
      "id": "jid0-XWJxt5VvCXkKzQK99PhZqAn7Xbg@jetpack"
    }
  },
  "default_locale": "en",
  "name": "__MSG_name__",
  "short_name": "__MSG_name__",
  "description": "__MSG_description__",
